#include<cstdio>
#include<iostream>
#include<cstring>
#include<algorithm>
using namespace std;
#define maxs 10000
typedef long long ll;
//容斥定理
int main(){
    ll n,m,n1,n2,n3,n4,n5,n6,n7,n8,n9,n10,n11,n12,n13,n14,n15;//可以不用定义这些变量直接用表达式就行，如n/2+n/3...........
    cin>>n;
    m=n;
    n1=n/2;
    n2=n/3;
    n3=n/5;
    n4=n/7;
    n5=n/6;
    n6=n/10;n7=n/14;n8=n/15;n9=n/21;n10=n/35;n11=n/30;n12=n/42;
    n13=n/70;n14=n/105;n15=n/210;
    n=n1+n2+n3+n4-n5-n6-n7-n8-n9-n10+n11+n12+n13+n14-n15;
    cout<<m-n<<endl;
	ret
